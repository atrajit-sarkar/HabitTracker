<?xml version="1.0" encoding="utf-8"?>
<!-- Multiple Overdue - Fiery anger gradient -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    
    <gradient
        android:angle="135"
        android:startColor="#FEE2E2"
        android:centerColor="#FCA5A5"
        android:endColor="#EF4444"
        android:type="linear" />
    
    <corners android:radius="20dp" />
    
    <stroke
        android:width="2dp"
        android:color="#DC2626" />
    
</shape>
